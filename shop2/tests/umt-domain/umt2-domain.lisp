           ((at-vehicle ?v ?ori)
            (not (hub ?ori))
            (hub ?des))
           ((:immediate !move-vehicle-nonroad-route2 ?v ?ori ?des ?r)))

        (:method (move-vehicle-non-road ?v ?ori ?des ?r)
           ((at-vehicle ?v ?ori)
            (hub ?ori)
            (not (hub ?des)))
           ((:immediate !move-vehicle-nonroad-route3 ?v ?ori ?des ?r)))
        
