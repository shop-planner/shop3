           ((at-vehicle ?v ?des))
           ((:immediate !!add-protection (at-vehicle ?v ?des)))
           ()
           ((:immediate !move-vehicle-road-route-crossCity ?v ?ori ?des ?c1 ?c2 ?r)))

        (:method (move-vehicle-non-road ?v ?ori ?des ?r)
